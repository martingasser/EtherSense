WorkingDirectory=/home/martin/devel/EtherSense
User=martin
Restart=always
RestartSec=5
ExecStart=/home/martin/venvs/magicqueen/bin/python3 -u /home/martin/devel/EtherSense/EtherSenseServer.py --plugins plugins.yolo plugins.surface --process_async

[Install]
